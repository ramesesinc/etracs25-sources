<schema adapter="police">
	<element tablename="cashreceipt_policeclearance" >
		<field name="objid" primary="true" /> 
		<field name="txndate" required="true"/> 
		<field name="txntype" required="true" caption="Txn Type"/> 
		<field name="receiptdate" required="true" caption="Receipt Date"/> 
		<field name="receiptno" required="true" caption="Receipt No" searchable="true"/> 
		<field name="amount" required="true" caption="Amount" type="decimal" width="100"/> 
		<field name="payer_objid" required="true"/> 
		<field name="payer_name" required="true"/> 
		<field name="paidby" required="true" caption="PaidBy"/> 
		<field name="paidbyaddress" required="true"/> 
		<field name="trackingno" required="true" searchable="true"/> 
		<field name="purpose" required="true"/> 
		<field name="voided" required="true"/> 
	</element> 
</schema> 
